  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#b0aaa2",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#9b6bb8"
      }
    },
    {
      "name": "Keyword Control",
      "scope": ["keyword.control", "keyword.operator.new", "keyword.operator.expression"],
      "settings": {
        "foreground": "#9b6bb8"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#5a9a52"
      }
    },
    {
      "name": "String Escape",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#4a9a9a"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "support.function", "meta.function-call"],
      "settings": {
        "foreground": "#5a7cc7"
      }
    },
    {
      "name": "Function Parameter",
      "scope": ["variable.parameter", "meta.parameter"],
      "settings": {
        "foreground": "#3d3a36"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#c78a5a"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant", "constant.language", "support.constant"],
      "settings": {
        "foreground": "#c78a5a"
      }
    },
    {
      "name": "Boolean",
      "scope": ["constant.language.boolean"],
      "settings": {
        "foreground": "#c78a5a"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#3d3a36"
      }
    },
    {
      "name": "Variable Property",
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": {
        "foreground": "#3d3a36"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "support.type", "entity.name.class"],
      "settings": {
        "foreground": "#4a9a9a"
      }
    },
    {
      "name": "Type Parameter",
      "scope": ["entity.name.type.parameter"],
      "settings": {
        "foreground": "#4a9a9a"
      }
    },
    {
      "name": "Interface",
      "scope": ["entity.name.type.interface"],
      "settings": {
        "foreground": "#4a9a9a"
      }
    },
    {
      "name": "Enum",
      "scope": ["entity.name.type.enum"],
      "settings": {
        "foreground": "#4a9a9a"
      }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#7c5cba"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#7c5cba"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator", "punctuation.accessor"],
      "settings": {
        "foreground": "#6b6660"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation", "meta.brace"],
      "settings": {
        "foreground": "#6b6660"
      }
    },
    {
      "name": "Bracket",
      "scope": ["punctuation.definition.block", "punctuation.section"],
      "settings": {
        "foreground": "#8a847c"
      }
    },
    {
      "name": "Title",
      "scope": ["entity.name.section", "markup.heading"],
      "settings": {
        "foreground": "#5a4090",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Link Text",
      "scope": ["string.other.link", "markup.underline.link"],
      "settings": {
        "foreground": "#7c5cba"
      }
    },
    {
      "name": "Link URL",
      "scope": ["markup.underline.link.markdown"],
      "settings": {
        "foreground": "#5a7cc7"
      }
    },
    {
      "name": "Emphasis",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strong",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Inline Code",
      "scope": ["markup.inline.raw", "markup.raw.inline"],
      "settings": {
        "foreground": "#5a9a52"
      }
    },
    {
      "name": "Code Block",
      "scope": ["markup.fenced_code.block", "markup.raw.block"],
      "settings": {
        "foreground": "#3d3a36"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#5a9a52"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#c75a4d"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#b8943a"
      }
    },
    {
      "name": "Regex",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#4a9a9a"
      }
    },
    {
      "name": "Decorator",
      "scope": ["meta.decorator", "punctuation.decorator"],
      "settings": {
        "foreground": "#9b6bb8"
      }
    },
    {
      "name": "Module",
      "scope": ["entity.name.module", "entity.name.namespace"],
      "settings": {
        "foreground": "#4a9a9a"
      }
    },
    {
      "name": "Import",
      "scope": ["keyword.control.import", "keyword.control.export", "keyword.control.from"],
      "settings": {
        "foreground": "#9b6bb8"
      }
    },
    {
      "name": "JSON Key",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#7c5cba"
      }
    },
    {
      "name": "CSS Property",
      "scope": ["support.type.property-name.css", "support.type.vendored.property-name.css"],
      "settings": {
        "foreground": "#5a7cc7"
      }
    },
    {
      "name": "CSS Value",
      "scope": ["support.constant.property-value.css"],
      "settings": {
        "foreground": "#c78a5a"
      }
    },
    {
      "name": "CSS Unit",
      "scope": ["keyword.other.unit.css"],
      "settings": {
        "foreground": "#c78a5a"
      }
    },
    {
      "name": "CSS Selector",
      "scope": ["entity.other.attribute-name.class.css", "entity.other.attribute-name.id.css"],
      "settings": {
        "foreground": "#7c5cba"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#c75a4d"
      }
    },
    {
      "name": "Deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#b8943a"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "function": "#5a7cc7",
    "function.declaration": "#5a7cc7",
    "method": "#5a7cc7",
    "method.declaration": "#5a7cc7",
    "variable": "#3d3a36",
    "variable.declaration": "#3d3a36",
    "variable.readonly": "#c78a5a",
    "parameter": "#3d3a36",
    "property": "#3d3a36",
    "property.declaration": "#3d3a36",
    "class": "#4a9a9a",
    "class.declaration": "#4a9a9a",
    "interface": "#4a9a9a",
    "interface.declaration": "#4a9a9a",
    "type": "#4a9a9a",
    "type.declaration": "#4a9a9a",
    "enum": "#4a9a9a",
    "enumMember": "#c78a5a",
    "namespace": "#4a9a9a",
    "module": "#4a9a9a",
    "keyword": "#9b6bb8",
    "comment": "#b0aaa2",
    "string": "#5a9a52",
    "number": "#c78a5a",
    "operator": "#6b6660",
    "decorator": "#9b6bb8",
    "macro": "#9b6bb8"
  }
