  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#686058",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#b890d8"
      }
    },
    {
      "name": "Keyword Control",
      "scope": ["keyword.control", "keyword.operator.new", "keyword.operator.expression"],
      "settings": {
        "foreground": "#b890d8"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#7cc070"
      }
    },
    {
      "name": "String Escape",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#68c0c0"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "support.function", "meta.function-call"],
      "settings": {
        "foreground": "#7a9ce0"
      }
    },
    {
      "name": "Function Parameter",
      "scope": ["variable.parameter", "meta.parameter"],
      "settings": {
        "foreground": "#e8e4df"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#e0a070"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant", "constant.language", "support.constant"],
      "settings": {
        "foreground": "#e0a070"
      }
    },
    {
      "name": "Boolean",
      "scope": ["constant.language.boolean"],
      "settings": {
        "foreground": "#e0a070"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#e8e4df"
      }
    },
    {
      "name": "Variable Property",
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": {
        "foreground": "#e8e4df"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "support.type", "entity.name.class"],
      "settings": {
        "foreground": "#68c0c0"
      }
    },
    {
      "name": "Type Parameter",
      "scope": ["entity.name.type.parameter"],
      "settings": {
        "foreground": "#68c0c0"
      }
    },
    {
      "name": "Interface",
      "scope": ["entity.name.type.interface"],
      "settings": {
        "foreground": "#68c0c0"
      }
    },
    {
      "name": "Enum",
      "scope": ["entity.name.type.enum"],
      "settings": {
        "foreground": "#68c0c0"
      }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#b898e0"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#b898e0"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator", "punctuation.accessor"],
      "settings": {
        "foreground": "#a8a29a"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation", "meta.brace"],
      "settings": {
        "foreground": "#a8a29a"
      }
    },
    {
      "name": "Bracket",
      "scope": ["punctuation.definition.block", "punctuation.section"],
      "settings": {
        "foreground": "#888078"
      }
    },
    {
      "name": "Title",
      "scope": ["entity.name.section", "markup.heading"],
      "settings": {
        "foreground": "#d8c0f8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Link Text",
      "scope": ["string.other.link", "markup.underline.link"],
      "settings": {
        "foreground": "#b898e0"
      }
    },
    {
      "name": "Link URL",
      "scope": ["markup.underline.link.markdown"],
      "settings": {
        "foreground": "#7a9ce0"
      }
    },
    {
      "name": "Emphasis",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strong",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Inline Code",
      "scope": ["markup.inline.raw", "markup.raw.inline"],
      "settings": {
        "foreground": "#7cc070"
      }
    },
    {
      "name": "Code Block",
      "scope": ["markup.fenced_code.block", "markup.raw.block"],
      "settings": {
        "foreground": "#e8e4df"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#7cc070"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#e07a6d"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#d8b85a"
      }
    },
    {
      "name": "Regex",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#68c0c0"
      }
    },
    {
      "name": "Decorator",
      "scope": ["meta.decorator", "punctuation.decorator"],
      "settings": {
        "foreground": "#b890d8"
      }
    },
    {
      "name": "Module",
      "scope": ["entity.name.module", "entity.name.namespace"],
      "settings": {
        "foreground": "#68c0c0"
      }
    },
    {
      "name": "Import",
      "scope": ["keyword.control.import", "keyword.control.export", "keyword.control.from"],
      "settings": {
        "foreground": "#b890d8"
      }
    },
    {
      "name": "JSON Key",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#b898e0"
      }
    },
    {
      "name": "CSS Property",
      "scope": ["support.type.property-name.css", "support.type.vendored.property-name.css"],
      "settings": {
        "foreground": "#7a9ce0"
      }
    },
    {
      "name": "CSS Value",
      "scope": ["support.constant.property-value.css"],
      "settings": {
        "foreground": "#e0a070"
      }
    },
    {
      "name": "CSS Unit",
      "scope": ["keyword.other.unit.css"],
      "settings": {
        "foreground": "#e0a070"
      }
    },
    {
      "name": "CSS Selector",
      "scope": ["entity.other.attribute-name.class.css", "entity.other.attribute-name.id.css"],
      "settings": {
        "foreground": "#b898e0"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#e07a6d"
      }
    },
    {
      "name": "Deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#d8b85a"
      }
    }
  ],
  "semanticHighlighting": true,
